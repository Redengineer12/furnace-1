<!DOCTYPE codename-titlestate >
<titlestate>
	<sprites folder="menus/titlescreen">
		<sprite name="logo" sprite="logo" x="0" y="0" />
		<sprite name="gf" sprite="menu" x="712" y="150">
			<anim name="danceLeft" anim="Símbolo 1" />
			<anim name="danceRight" anim="Símbolo 1" />
		</sprite>

		<titleEnter sprite="titleEnter" x="-150" y="-100" />
	</sprites>

	<intro length="16">
		<text beat="1">
			<line text="rxdialmusic" />
			<line text="mattmoney" />
			<line text="DestrioSZ" />
			<line text="Sykley" />
		</text>

		<text beat="3">
			<line text="rxdialmusic" />
			<line text="mattmoney" />
